{
  "name": "instagram-clone-with-react",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@babel/core": "^7.19.6",
    "@tailwindcss/typography": "^0.5.7",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^13.0.0",
    "@testing-library/user-event": "^13.2.1",
    "@welldone-software/why-did-you-render": "^7.0.1",
    "date-fns": "^2.29.3",
    "eslint-config-react-app": "^7.0.1",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.10",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslintrc": "^1.0.6",
    "eslintrc.json": "^0.5.6",
    "firebase": "^9.9.4",
    "ls": "^0.2.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-loading-skeleton": "^3.1.0",
    "react-router-dom": "^6.4.0",
    "react-scripts": "^5.0.1",
    "web-vitals": "^2.1.0"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">= 1.19.1"
  },
  "settings": {
    "react": {
      "version": "detect"
    }
  },
  "scripts": {
    "build:css": "postcss src/styles/tailwind.css -o src/styles/app.css",
    "watch:css": "postcss src/styles/tailwind.css -o src/styles/app.css --watch",
    "react-scripts:start": "sleep 5 && react-scripts start",
    "react-scripts:dist": "react-scripts build",
    "start": "run-p watch:css react-scripts:start",
    "build": "run-s build:css react-scripts:dist",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "lint": "eslint . --ext js,jsx,ts,tsx --fix"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "rules": {
      "no-unused-vars": "warn"
    },
    "overrides": [
      {
        "files": ["**/*.ts?(x)"],
        "rules": {
          "max-len": "warn"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "autoprefixer": "^10.4.11",
    "babel-eslint": "^7.2.3",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "npm-run-all": "^4.1.5",
    "postcss-cli": "^10.0.0",
    "prettier": "2.7.1",
    "prop-types": "^15.8.1",
    "tailwindcss": "^3.1.8"
  },
  "main": "index.js",
  "repository": "https://github.com/NiniLiao/instagram-clone-with-react.git",
  "author": "<u9513031@cmu.edu.tw>",
  "license": "MIT"
}
